{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "no-unused-expression": [
    true,
    "allow-fast-null-checks"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "no-string-literal": false,
    "no-unused-expression": false,
    "object-literal-sort-keys": false,
    "no-empty": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  }
}